{
    "name": "j0k3r/f43.me",
    "description": "A More readable & cleaner feed",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jeremy Benoist",
            "email": "jeremy.benoist@gmail.com",
            "homepage": "http://www.j0k3r.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "beberlei/doctrineextensions": "^1.2",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^3.5",
        "gedmo/doctrine-extensions": "^3.6.0",
        "j0k3r/graby": "dev-master",
        "j0k3r/graby-site-config": "^1",
        "j0k3r/php-imgur-api-client": "~4.0",
        "laminas/laminas-code": "^4.5",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/httplug-bundle": "^2.1",
        "ricardoper/twitteroauth": "~1.0",
        "sentry/sentry-symfony": "^5.0",
        "simplepie/simplepie": "^1.8",
        "stof/doctrine-extensions-bundle": "~1.1",
        "symfony/amqp-messenger": "*",
        "symfony/asset": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^2.8",
        "symfony/form": "*",
        "symfony/lock": "*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/runtime": "*",
        "symfony/security-bundle": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/webpack-encore-bundle": "^2.3",
        "symfony/yaml": "*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friendsofphp/php-cs-fixer": "~3.0",
        "php-http/mock-client": "^1.3",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^11.0",
        "rector/rector": "^2.1",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/phpunit-bridge": "*",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.27"
        },
        "bin-dir": "bin",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.4.*",
            "docker": true
        }
    }
}
